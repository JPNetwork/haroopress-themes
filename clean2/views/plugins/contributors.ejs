    <% if(contributors.display) { %>
    <div class="well contributors">
        <h6 class="title"><a href="/authors">contributors</a></h6>
        <% for(var id in authors) {
            var author = authors[id];
        %>
        <div class="row-fluid contributor">
            <a href="/authors/<%= author.head.name %>" title=""><img src="<%= author._gravatar %>" /></a>
            <p>
                <strong><%= author.head.name %></strong><br>
                <span>@<%= author.head.twitter %></span>
            </p>
        </div>
        <% } %>
    </div>
    <% } %>