<!-- Masthead (blueprinty thing)
================================================== -->
<header class="jumbotron masthead" id="overview">
    <div class="inner">
    </div>
</header>


<div class="container">
    <div class="row">
        <div class="span12">
            <div class="subnav">
                <ul class="nav nav-pills">
                </ul>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="span10">
            <div class="well markdown bg">
                <div class="pull-right">
                    <%- partial('plugins/facebook', config.plugins.facebook) %>
                </div>

                <div class="pull-right">
                    <%- partial('plugins/google-plus', config.plugins.google) %>
                </div>

                <div class="pull-right">
                    <%- partial('plugins/twitter', config.plugins.twitter) %>
                </div>
                <%- page.html %>
            </div>
        </div>

        <div class="span2">
            <h3> * Spread Social World</h3>
            <ul class="unstyled share">
                <li class="twitter">
                    <a href="https://twitter.com/share" class="twitter-share-button">Tweet</a>
                    <script type="text/javascript" src="//platform.twitter.com/widgets.js"></script>
                </li>
                <li class="facebook">
                    <div id="fb-root"></div>
                    <script>(function(d, s, id) {
                      var js, fjs = d.getElementsByTagName(s)[0];
                      if (d.getElementById(id)) {return;}
                      js = d.createElement(s); js.id = id;
                      js.src = "//connect.facebook.net/en_US/all.js#xfbml=1";
                      fjs.parentNode.insertBefore(js, fjs);
                    }(document, 'script', 'facebook-jssdk'));</script>
                    <div class="fb-like" data-send="false" data-layout="button_count" data-width="450" data-show-faces="true"></div>
                </li>
                <li class="google">
                    <script type="text/javascript" src="https://apis.google.com/js/plusone.js"></script>
                    <g:plusone size="medium"></g:plusone>
                </li>
            </ul>
        </div>
    </div>
</div><!-- /container -->
